{
  "kind": "collectionType",
  "collectionName": "arts",
  "info": {
    "name": "Art",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "Title": {
      "type": "string"
    },
    "Description": {
      "type": "richtext"
    },
    "Pictures": {
      "collection": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "Artist": {
      "via": "Arts",
      "model": "artist"
    },
    "Articul": {
      "type": "string",
      "required": false,
      "unique": false
    },
    "Materials": {
      "type": "text"
    },
    "Year": {
      "type": "date"
    },
    "Owners_price": {
      "type": "integer",
      "private": true
    },
    "wall": {
      "model": "wall",
      "via": "arts"
    },
    "slug": {
      "type": "string"
    },
    "styles": {
      "collection": "style",
      "via": "arts",
      "dominant": true
    },
    "Price": {
      "type": "integer"
    },
    "subjects": {
      "collection": "subject",
      "via": "arts",
      "dominant": true
    },
    "width": {
      "type": "decimal",
      "required": false
    },
    "height": {
      "type": "decimal",
      "required": false
    },
    "square": {
      "type": "decimal"
    },
    "isSquare": {
      "type": "boolean"
    },
    "mediums": {
      "collection": "medium",
      "via": "arts"
    },
    "main": {
      "type": "boolean"
    },
    "sold": {
      "type": "boolean",
      "default": false
    },
    "video": {
      "collection": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "views": {
      "type": "integer",
      "default": 0,
      "min": 0
    },
    "user_uploader": {
      "plugin": "users-permissions",
      "model": "user",
      "via": "created_arts"
    }
  }
}
